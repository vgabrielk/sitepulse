APP_NAME=SitePulse
APP_ENV=production
APP_DEBUG=false
APP_URL=https://your-domain.com

# Database - FORÇAR MYSQL
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=analytics
DB_USERNAME=root
DB_PASSWORD=123

# SitePulse específico
SITEPULSE_WIDGET_URL=https://your-domain.com/widget
SITEPULSE_RATE_LIMIT_PER_MINUTE=60
SITEPULSE_ANONYMIZE_IPS=true
SITEPULSE_REQUIRE_HTTPS=true

# Cache - usar file em vez de database
CACHE_STORE=file
SESSION_DRIVER=file
QUEUE_CONNECTION=sync